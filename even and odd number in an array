// PROGRAM TO FIND OUT EVEN AND ODD NUMBER IN ARRAY
#include <stdio.h>

int main(){
    int n;
    scanf("%d",&n);//declare the size of an array
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);//declare the array
    }
    printf("even number in array is:");
    for(int i=0;i<n;i++){
        if(arr[i]%2==0)
        printf("%d",arr[i]);
    }
    printf("odd number in array is:");
    for(int i=0;i<n;i++){
        
    
    if(arr[i]%2!=0)
    printf("%d",arr[i]);
    }
    return 0;
}
