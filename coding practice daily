#include <stdio.h>

int main(){
    int a,b,c;
    printf("enter the value of a\n");
    scanf("%d",&a);
    printf("enter the value ofb\n");
    scanf("%d",&b);
      printf("enter the value ofc\n");
    scanf("%d",&c);
    if(a>b && a>c){
        printf("largest number is %d",a);
    }
    if(b>a && b>c){
        printf("largest number is %d",b);
    }
    if(c>a && c>b){
        printf("largest number is %d",c);
    }
    return 0;
}
