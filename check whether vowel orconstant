// PROGRAM TO CHECK WHETHER A CHARACTER IS CONSTANAT OR VOWEL
#include <stdio.h>
int main(){
    char ch='u';
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U'){
        printf("the character is vowel %c",ch);
    }
    else{
        printf("the character is constant %c",ch);
    }
    return 0;
}
