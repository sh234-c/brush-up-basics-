#include <bits/stdc++.h>
#include <unordered_set>
using namespace std;
int main(){
    unordered_set<int>s;//initialised the set
    //insert the element into set
    s.insert(10);
    s.insert(11);
    s.insert(12);
    s.insert(3);
    s.insert(3);//unorederd set does not allow duplicacy
    for(auto it=s.begin();it!=s.end();it++)
        cout<<(*it)<<endl;
        cout<<"number of element:"<<s.size()<<endl;
        // s.clear();//whole set clear
        //  cout<<"number of element:"<<s.size()<<endl;
         int key=10;
         if(s.find(key)==s.end()){
             cout<<"key not found"<<endl;
         }
         else{
            cout<<"key  found"<<endl;
         }
         int deleted_key=3;
         s.erase(deleted_key);
            for(auto it=s.begin();it!=s.end();it++)
        cout<<(*it)<<endl;
           cout<<"number of element:"<<s.size()<<endl;
           if(s.count(10)){
               cout<<"key found"<<endl;
           }
               else{
                   cout<<"key not found"<<endl;
               }
               
           }
