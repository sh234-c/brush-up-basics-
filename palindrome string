// PALINDROME QUESTION
//palindrome string is a string whose reverse string iis same as  previous string
#include <stdio.h>
#include <string.h>
int main(){
    char s[100]="naman";
    
    int l=0;
    int h=strlen(s)-1;
    while(l<h){
        if(s[l++]!=s[h--]){
            printf("%s is not palindrome\n",s);
            return 0;
        }
    }
    printf("%s is palindrome\n",s);
    return 0;
    
}
