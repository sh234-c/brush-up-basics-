#include <stdio.h>

int main(){
    int marks;
    printf("enter your marks\n");
    scanf("%d",&marks);
    if(marks>=90 && marks<=100){
        printf("the grade is A");
    }
    else if(marks>=80 && marks<=90){
        printf("the grade is B");
    }
    else if(marks>=70 && marks<=80){
        printf("the grade is C");
    }
    else{
        printf("the grade is below C");
    }
    return 0;
}
