// FIND THE SMALLEST ELEMENT IN ARRAY
#include <stdio.h>
int returnMin(int arr[],int n){
    int min=arr[0];//assigh 1 element in array element
    for(int i=0;i<n;i++){
        if(arr[i]<min){
            min=arr[i];
        }
    }
    return min;
}
int main(){
    int arr[]={1,2,3,4,5};
    int min=returnMin(arr,5);
    printf("the min element in array is %d:",min);
    return 0;
}
