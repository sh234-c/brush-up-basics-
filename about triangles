#include <stdio.h>

int main(){
    int a,b,c;//three sides of the triangle
    printf("enter the value of a\n");
    scanf("%d",&a);
     printf("enter the value of b\n");
    scanf("%d",&b);
     printf("enter the value of c\n");
    scanf("%d",&c);
    if((a==b) && (b==c)){
        printf("equilateral triangle");
    }
    else if((a==b) || (b==c) || (a==c)){
        printf("isosceles triangle");
    }
    else{
        printf("scalar triangle");
    }
    return 0;
}
